.sam {
    width: 200px;
    height: 200px;
    background-color: rgb(255, 255, 255);
    transition-property: translate;
    transition-duration: 1s;
    transition-timing-function: ease-in;
    position: absolute;
    left: 900px;
    top: 300px;
}

.EIn {
    transition-timing-function: ease-out-sine;
    transition-duration: 1s;
    position: absolute;
    left: 1000px;
    top: 300px;
    transition-property: position;
}

